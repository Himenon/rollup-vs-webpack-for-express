{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "sourceMap": false,
    "target": "es2020",
    "lib": ["es2020"],
    "rootDir": ".",
    "strict": true
  }
}
